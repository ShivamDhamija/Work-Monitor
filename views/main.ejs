<%- include("partials/header") %>
<body class="section1">
<h1 class="heading">Check List</h1>
<!--header-->
<br><br><br>
<form method="post">
    <button formaction="/logOut">Log-out</button>
    <button formaction="/forgotPassword">Change Password</button>
</form>
<br><br>
<button id="newListbtn" name="" class="" >New List</button>
<button id="myListbtn" name="" class="">MY List</button>
<button id="completedWorkbtn" name="" class="">Completed Work</button>
<button id="missedWorkbtn" name="" class="">Missed Work</button>
<br><br>
<!--style="display: none;" for removing element from screen use  it-->
<div id="newList" class="center" name="newList" <%if(showdiv==="newList"){%>style="display:inline"<%}else{%>style="display:none"<%}%> >
    <h1>NEW-List</h1>
    <%if(userDetail.userType==="daily"){%>
        <div name="daily">
            <%-include("partials/dailyNewList")%>
        </div>
    <%}else{%>
        <div name="professional">
            <%-include("partials/professionalNewList")%>
        </div>
    <%}%>    
</div>

<!--temprary stoping working of these divs-->

<div id="myList" class="" name="myList" <%if(showdiv==="myList"){%>style="display:inline"<%}else{%>style="display:none"<%}%>>
    <h1>MY-LIST</h1>
    <%if(userDetail.userType==="daily"){%>
        <div name="daily">
            <%-include("partials/dailyMyList",{list:task})%>
        </div>
    <%}else{%>
        <div name="professional">
            <%-include("partials/professionalMyList",{list:task})%>
        </div>
    <%}%>
</div>
<div id="completedWork" class="" name="completedWork" <%if(showdiv==="completedWork"){%>style="display:inline"<%}else{%>style="display:none"<%}%>>
    <h1>COMPLETD-WORK</h1>
    <%if(userDetail.userType==="daily"){%>
        <div name="daily">
            <%-include("partials/dailyCompletedWork",{listOfWork:completedWork})%>
        </div>
    <%}else{%>
        <div name="professional">
            <%-include("partials/professionalCompletedWork",{listOfWork:completedWork})%>
        </div>
    <%}%>
</div>
<div id="missedWork" class="" name="missedWork" <%if(showdiv==="missedWork"){%>style="display:inline"<%}else{%>style="display:none"<%}%>>
    <h1>MISSED-WORK</h1>
    <%if(userDetail.userType==="daily"){%>
        <div name="daily">
            <%-include("partials/dailyMissedWork",{listOfWork:inCompletedWork})%>
        </div>
    <%}else{%>
        <div name="professional">
            <%-include("partials/professionalMissedWork",{listOfWork:inCompletedWork})%>
        </div>
    <%}%>
</div>

<script type="module" src="frontJsFile/divShow.js"></script>
<!--footer-->